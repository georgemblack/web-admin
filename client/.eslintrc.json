{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "prettier/vue",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "vue/require-prop-types": ["off"],
    "vue/html-self-closing": ["off"]
  }
}
